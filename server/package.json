{
  "name": "financeful-server",
  "version": "1.0.0",
  "description": "Backend for financeful",
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronvonkreisler/financeful"
  },
  "scripts": {
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "dev:local": "dotenv -e .env.local -- yarn dev",
    "dev:local:debug": "dotenv -e .env.local --debug -- yarn dev",
    "build": "tsc",
    "migrate": "yarn prisma migrate dev --preview-feature",
    "migrate:local": "dotenv -e .env.local -- yarn migrate",
    "migrate:test": "dotenv -e .env.test -- yarn prisma migrate dev --name init",
    "studio": "dotenv -e .env.local -- yarn prisma studio",
    "seed:db": "yarn prisma db seed --preview-feature",
    "seed:db:local": "dotenv -e .env.local -- yarn seed:db",
    "reset:db": "yarn prisma migrate reset --skip-seed",
    "docker:testdb": "docker compose -f docker-compose.test.yml up -d",
    "test:unit": "jest -i --testPathPattern=src/__tests__/unit",
    "test:unit:watch": "jest -i --watch --testPathPattern=src/__tests__/unit",
    "test:integration": "yarn docker:testdb && yarn migrate:test && dotenv -e .env.test -- jest -i --testPathPattern=src/__tests__/integration",
    "test:integration:watch": "yarn docker:testdb && yarn migrate:test && dotenv -e .env.test -- jest -i --watch --testPathPattern=src/__tests__/integration",
    "kill:tests": "docker compose -f docker-compose.test.yml down"
  },
  "keywords": [
    "Personal Finance",
    "GraphQL",
    "Node",
    "TypeScript"
  ],
  "author": "Aaron von Kreisler",
  "license": "BSD",
  "_moduleAliases": {
    "@Modules": "dist/modules",
    "@Lib": "dist/lib",
    "@Shared": "dist/shared"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.34",
    "@types/prompt": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-plugin-node": "^11.1.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.14",
    "mockdate": "^3.0.5",
    "prettier": "^2.2.1",
    "prisma": "^2.25.0",
    "prompt": "^1.1.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.25.0",
    "apollo-datasource": "^0.7.3",
    "apollo-server-express": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "dotenv-cli": "^4.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nanoid": "^3.1.31",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
